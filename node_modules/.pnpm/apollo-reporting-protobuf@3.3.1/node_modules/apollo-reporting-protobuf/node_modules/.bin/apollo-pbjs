#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-reporting-protobuf@3.3.1/node_modules/apollo-reporting-protobuf/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-reporting-protobuf@3.3.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-reporting-protobuf@3.3.1/node_modules/apollo-reporting-protobuf/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-reporting-protobuf@3.3.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/pbjs" "$@"
else
  exec node  "$basedir/../../../../../@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/pbjs" "$@"
fi
