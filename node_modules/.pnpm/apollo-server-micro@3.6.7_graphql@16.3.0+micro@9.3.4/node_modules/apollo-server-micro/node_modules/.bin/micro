#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules/micro/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules/micro/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-server-micro@3.6.7_graphql@16.3.0+micro@9.3.4/node_modules/apollo-server-micro/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-server-micro@3.6.7_graphql@16.3.0+micro@9.3.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules/micro/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules/micro/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/micro@9.3.4/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-server-micro@3.6.7_graphql@16.3.0+micro@9.3.4/node_modules/apollo-server-micro/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/apollo-server-micro@3.6.7_graphql@16.3.0+micro@9.3.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../micro@9.3.4/node_modules/micro/bin/micro.js" "$@"
else
  exec node  "$basedir/../../../../../micro@9.3.4/node_modules/micro/bin/micro.js" "$@"
fi
