#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules/xss/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules/xss/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/graphql-playground-html/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules/xss/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules/xss/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/xss@1.0.11/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/graphql-playground-html/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules/@apollographql/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/.pnpm/@apollographql+graphql-playground-html@1.6.29/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../xss@1.0.11/node_modules/xss/bin/xss" "$@"
else
  exec node  "$basedir/../../../../../../xss@1.0.11/node_modules/xss/bin/xss" "$@"
fi
