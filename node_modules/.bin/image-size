#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jeremy/Documents/Projects/playprisma/node_modules/image-size/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/image-size/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jeremy/Documents/Projects/playprisma/node_modules/image-size/bin/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules/image-size/node_modules:/Users/jeremy/Documents/Projects/playprisma/node_modules:/Users/jeremy/Documents/Projects/node_modules:/Users/jeremy/Documents/node_modules:/Users/jeremy/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../image-size/bin/image-size.js" "$@"
else
  exec node  "$basedir/../image-size/bin/image-size.js" "$@"
fi
